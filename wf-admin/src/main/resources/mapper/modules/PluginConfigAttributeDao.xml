<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hy.wf.admin.modules.dao.PluginConfigAttributeDao">
	
	<select id="queryPluginNameList" resultType="java.util.HashMap">
		select id as 'key',plugin_name as 'value'  from t_plugin_config where is_enabled = 1
	</select>

</mapper>